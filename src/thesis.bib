  author = {Lu, Rui and Wang, Ping and Parton, Trevor and Zhou, Yang and Chrysovergis, Kaliopi and Rockowitz, Shira and Chen, Wei-Yi and {Abdel-Wahab}, Omar and Wade, Paul A. and Zheng, Deyou and Wang, Gang Greg},
