  author = {Li, Kailong and Zhang, Yuannyu and Liu, Xin and Liu, Yuxuan and Gu, Zhimin and Cao, Hui and Dickerson, Kathryn E. and Chen, Mingyi and Chen, Weina and Shao, Zhen and Ni, Min and Xu, Jian},
