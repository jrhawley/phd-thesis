@article{serraAutomaticAnalysis3Dmodelling2017,
  title = {Automatic Analysis and {{3D-modelling}} of {{Hi-C}} Data Using {{TADbit}} Reveals Structural Features of the Fly Chromatin Colors},
  author = {Serra, Fran{\c c}ois and Ba{\`u}, Davide and Goodstadt, Mike and Castillo, David and Filion, Guillaume J. and {Marti-Renom}, Marc A.},
