@article{jiangEnvironmentallyResponsiveGenomewide2014,
  title = {Environmentally Responsive Genome-Wide Accumulation of de Novo {{Arabidopsis}} Thaliana Mutations and Epimutations},
  author = {Jiang, Caifu and Mithani, Aziz and Belfield, Eric J. and Mott, Richard and Hurst, Laurence D. and Harberd, Nicholas P.},
  year = {2014},
  month = jan,
  journal = {Genome Research},
  volume = {24},
  number = {11},
  pages = {1821--1829},
  issn = {1088-9051, 1549-5469},
  doi = {10/f6ns7v},
  abstract = {Evolution is fueled by phenotypic diversity, which is in turn due to underlying heritable genetic (and potentially epigenetic) variation. While environmental factors are well known to influence the accumulation of novel variation in microorganisms and human cancer cells, the extent to which the natural environment influences the accumulation of novel variation in plants is relatively unknown. Here we use whole-genome and whole-methylome sequencing to test if a specific environmental stress (high-salinity soil) changes the frequency and molecular profile of accumulated mutations and epimutations (changes in cytosine methylation status) in mutation accumulation (MA) lineages of Arabidopsis thaliana. We first show that stressed lineages accumulate {$\sim$}100\% more mutations, and that these mutations exhibit a distinctive molecular mutational spectrum (specific increases in relative frequency of transversion and insertion/deletion [indel] mutations). We next show that stressed lineages accumulate {$\sim$}45\% more differentially methylated cytosine positions (DMPs) at CG sites (CG-DMPs) than controls, and also show that while many ({$\sim$}75\%) of these CG-DMPs are inherited, some can be lost in subsequent generations. Finally, we show that stress-associated CG-DMPs arise more frequently in genic than in nongenic regions of the genome. We suggest that commonly encountered natural environmental stresses can accelerate the accumulation and change the profiles of novel inherited variants in plants. Our findings are significant because stress exposure is common among plants in the wild, and they suggest that environmental factors may significantly alter the rates and patterns of incidence of the inherited novel variants that fuel plant evolution.},
  langid = {english},
  pmid = {25314969},
  keywords = {\#high,DNAme,epimutations,mutations,plants,selection},
  file = {/Users/hawleyj/Documents/Zotero/storage/7V3XLVZR/Jiang et al_2014_Environmentally responsive genome-wide accumulation of de novo Arabidopsis.pdf}
}

