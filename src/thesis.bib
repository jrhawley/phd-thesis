  author = {Yen, Ray-Whay Chiu and Vertino, Paula M and Nelkin, Barry D and Yu, Jane J and {El-Deiry}, Wafik and Cumaraswamy, Arunthathi and Lennon, Gregory G and Trask, Barbara J and Celano, Paul and Baylin, Stephen B},
