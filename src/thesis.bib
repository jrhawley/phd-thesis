@article{hauserHeterogeneityBackgroundFitness2014,
  title = {Heterogeneity in Background Fitness Acts as a Suppressor of Selection},
  author = {Hauser, Oliver P. and Traulsen, Arne and Nowak, Martin A.},
  year = {2014},
  month = feb,
  journal = {Journal of Theoretical Biology},
  volume = {343},
  pages = {178--185},
  issn = {00225193},
  doi = {10.1016/j.jtbi.2013.10.013},
  abstract = {We introduce the concept of heterogeneity in background fitness to evolutionary dynamics in finite populations. Background fitness is specific to an individual but not linked to its strategy. It can be thought of as a property that is related to the physical or societal position of an individual, but is not dependent on the strategy that is adopted in the evolutionary process under consideration. In our model, an individual's total fitness is the sum of its background fitness and the fitness derived from using a specific strategy. This approach has important implications for the imitation of behavioural strategies: if we imitate others for their success, but can only adopt their behaviour and not their social and economic ties, we may imitate in vain. We study the effect of heterogeneity in background fitness on the fixation of a mutant strategy with constant fitness. We find that heterogeneity suppresses selection, but also decreases the time until a novel strategy either takes over the population or is lost again. We derive analytical solutions of the fixation probability in small populations. In the case of large total background fitness in a population with maximum inequality, we find a particularly simple approximation of the fixation probability. Numerical simulations suggest that this simple approximation also holds for larger population sizes.},
  langid = {english},
  keywords = {\#med},
  file = {/Users/hawleyj/Documents/Zotero/storage/9M5SAVUU/Hauser et al. - 2014 - Heterogeneity in background fitness acts as a supp.pdf}
}

@unpublished{hawleyEpigeneticDynamicsUnderlying2021,
  title = {Epigenetic Dynamics Underlying {{B}} Cell Acute Lymphoblastic Leukemia Relapse},
  author = {Hawley, James and {Garcia-Prat}, Laura and Murison, Alexander and {Chan-Seng-Yue}, Michelle and Dobson, Stephanie M. and Vanner, Robert J. and Waanders, Esm{\'e} and McLeod, Jessica and Gan, Olga I. and Grandal, Ildiko and Xie, Stephanie Z. and Hosseini, Mohsen and Olsen, Scott R. and Neale, Geoffrey and Chan, Steven M. and Easton, John and Guidos, Cynthia J. and Danska, Jayne S. and Minden, Mark D. and Mullighan, Charles G. and Lupien, Mathieu and Dick, John E.},
  year = {2021},
  abstract = {Relapse of B-ALL remains a significant cause of death in treating the disease. Genomic investigations indicate that relapsed disease often arises from a minor clone of cells present at diagnosis. However, both genetic and epigenetic variation have been observed in B-ALL and other leukemias, and why come cells with particular genetic or epigenetic profiles survive therapy remains unknown. Here, we use targeted genome sequencing, RNA-seq, ATAC-seq, and bisulfite sequencing of patient-matched samples with PDXs to investigate the dynamics of the genome and epigenome over B-ALL relapse. We find that \textbackslash glsentrylong\{dname\} profiles most closely resemble genetic clones at diagnosis and relapse. Moreover, we find widespread increases to \textbackslash glsentrylong\{dname\} at relapse, mirroring a more stem-like phenotype. This work suggests that therapy selects for clones with stem-like characteristics, both genetically and epigenetically in \textbackslash glsentrylong\{ball\}.}
}

