@article{grunewaldEwingSarcoma2018,
  title = {Ewing Sarcoma},
  author = {Gr{\"u}newald, Thomas G. P. and {Cidre-Aranaz}, Florencia and Surdez, Didier and Tomazou, Eleni M. and {de {\'A}lava}, Enrique and Kovar, Heinrich and Sorensen, Poul H. and Delattre, Olivier and Dirksen, Uta},
