  langid = {english},
  keywords = {3d_genome,Chromosomes,differential_analysis,hic,hichip,methods,Software,Statistical methods},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Chromosomes;Software;Statistical methods Subject\_term\_id: chromosomes;software;statistical-methods}
