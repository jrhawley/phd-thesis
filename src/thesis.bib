@article{varamballyPolycombGroupProtein2002,
  title = {The Polycomb Group Protein {{EZH2}} Is Involved in Progression of Prostate Cancer},
  author = {Varambally, Sooryanarayana and Dhanasekaran, Saravana M. and Zhou, Ming and Barrette, Terrence R. and {Kumar-Sinha}, Chandan and Sanda, Martin G. and Ghosh, Debashis and Pienta, Kenneth J. and Sewalt, Richard G. A. B. and Otte, Arie P. and Rubin, Mark A. and Chinnaiyan, Arul M.},
