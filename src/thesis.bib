  author = {Rhee, Ina and Jair, Kam-Wing and Yen, Ray-Whay Chiu and Lengauer, Christoph and Herman, James G and Kinzler, Kenneth W and Vogelstein, Bert and Baylin, Stephen B. and Schuebel, Kornel},
