  author = {Kleeff, Jorg and Korc, Murray and Apte, Minoti and La Vecchia, Carlo and Johnson, Colin D. and Biankin, Andrew V. and Neale, Rachel E. and Tempero, Margaret and Tuveson, David A. and Hruban, Ralph H. and Neoptolemos, John P.},
  year = {2016},
