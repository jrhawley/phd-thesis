  langid = {english},
  keywords = {Cancer genomics,chromatin,Chromatin analysis,Computational models,Neural stem cells,single_cell,Transposition,velocity},
  annotation = {Bandiera\_abtest: a Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Cancer genomics;Chromatin analysis;Computational models;Neural stem cells;Transposition Subject\_term\_id: cancer-genomics;chromatin-analysis;computational-models;neural-stem-cells;transposition}
}

@article{tehComparisonMethylcaptureSequencing2016,
  title = {Comparison of {{Methyl-capture Sequencing}} vs. {{Infinium 450K}} Methylation Array for Methylome Analysis in Clinical Samples},
  author = {Teh, Ai Ling and Pan, Hong and Lin, Xinyi and Lim, Yubin Ives and Patro, Chinari Pawan Kumar and Cheong, Clara Yujing and Gong, Min and MacIsaac, Julia L. and Kwoh, Chee-Keong and Meaney, Michael J and Kobor, Michael S and Chong, Yap-Seng and Gluckman, Peter D and Holbrook, Joanna D and Karnani, Neerja},
