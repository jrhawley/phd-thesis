  langid = {english},
  keywords = {CTCF,inflammation,regulation},
  file = {/Users/hawleyj/Documents/Zotero/storage/C92UG4P8/Stik et al. - 2020 - CTCF is dispensable for immune cell transdifferent.pdf}
}

