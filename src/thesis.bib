  author = {Wang, Yunhao and Wang, Anqi and Liu, Zujun and Thurman, Andrew L. and Powers, Linda S. and Zou, Meng and Zhao, Yue and Hefel, Adam and Li, Yunyi and Zabner, Joseph and Au, Kin Fai},
  year = {2019},
  month = jan,
  journal = {Genome Research},
