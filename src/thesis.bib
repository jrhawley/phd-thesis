  author = {Kim, Tae Hoon and Abdullaev, Ziedulla K. and Smith, Andrew D. and Ching, Keith A. and Loukinov, Dmitri I. and Green, Roland D. and Zhang, Michael Q. and Lobanenkov, Victor V. and Ren, Bing},
