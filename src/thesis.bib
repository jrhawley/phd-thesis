@article{garciapratTFEBmediatedEndolysosomalActivity2021,
  title = {{{TFEB-mediated}} Endolysosomal Activity Controls Human Hematopoietic Stem Cell Fate},
  author = {{Garc{\'i}a-Prat}, Laura and Kaufmann, Kerstin B. and Schneiter, Florin and Voisin, Veronique and Murison, Alex and Chen, Jocelyn and {Chan-Seng-Yue}, Michelle and Gan, Olga I. and McLeod, Jessica L. and Smith, Sabrina A. and Shoong, Michelle C. and Parris, Darrien and Pan, Kristele and Zeng, Andy G. X. and Krivdova, Gabriela and Gupta, Kinam and Takayanagi, Shin-Ichiro and Wagenblast, Elvin and Wang, Weijia and Lupien, Mathieu and Schroeder, Timm and Xie, Stephanie Z. and Dick, John E.},
  year = {2021},
  month = oct,
  journal = {Cell Stem Cell},
  volume = {28},
  number = {10},
  pages = {1838-1850.e10},
  publisher = {{Elsevier}},
  issn = {1934-5909, 1875-9777},
  doi = {10.1016/j.stem.2021.07.003},
  abstract = {{$<$}h2{$>$}Summary{$<$}/h2{$><$}p{$>$}It is critical to understand how human quiescent long-term hematopoietic stem cells (LT-HSCs) sense demand from daily and stress-mediated cues and then transition into bioenergetically active progeny to differentiate and meet these cellular needs. However, the demand-adapted regulatory circuits of these early steps of hematopoiesis are largely unknown. Here we show that lysosomes, sophisticated nutrient-sensing and signaling centers, are regulated dichotomously by transcription factor EB (TFEB) and MYC to balance catabolic and anabolic processes required for activating LT-HSCs and guiding their lineage fate. TFEB-mediated induction of the endolysosomal pathway causes membrane receptor degradation, limiting LT-HSC metabolic and mitogenic activation, promoting quiescence and self-renewal, and governing erythroid-myeloid commitment. In contrast, MYC engages biosynthetic processes while repressing lysosomal catabolism, driving LT-HSC activation. Our study identifies TFEB-mediated control of lysosomal activity as a central regulatory hub for proper and coordinated stem cell fate determination.{$<$}/p{$>$}},
  langid = {english},
  pmid = {34343492},
  keywords = {differentiation,h\&c,TFEB}
}

