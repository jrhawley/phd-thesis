  keywords = {Base Sequence,Bias,bias-correction,Gene Expression Profiling,gene-length,Humans,methods,Oligonucleotide Array Sequence Analysis,phd-thesis,RNA-seq,RNA; Messenger,Systems Biology,Transcription; Genetic},
  file = {/Users/hawleyj/Documents/Zotero/storage/K8XE262E/Oshlack and Wakefield - 2009 - Transcript length bias in RNA-seq data confounds s.pdf}
}

