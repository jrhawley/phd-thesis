@article{verhoevenStressinducedDNAMethylation2010,
  title = {Stress-Induced {{DNA}} Methylation Changes and Their Heritability in Asexual Dandelions},
  author = {Verhoeven, Koen J. F. and Jansen, Jeroen J. and van Dijk, Peter J. and Biere, Arjen},
  year = {2010},
  month = mar,
  journal = {New Phytologist},
  volume = {185},
  number = {4},
  pages = {1108--1118},
  issn = {1469-8137},
  doi = {10/dwdj7b},
  abstract = {\textbullet{} DNA methylation can cause heritable phenotypic modifications in the absence of changes in DNA sequence. Environmental stresses can trigger methylation changes and this may have evolutionary consequences, even in the absence of sequence variation. However, it remains largely unknown to what extent environmentally induced methylation changes are transmitted to offspring, and whether observed methylation variation is truly independent or a downstream consequence of genetic variation between individuals. \textbullet{} Genetically identical apomictic dandelion (Taraxacum officinale) plants were exposed to different ecological stresses, and apomictic offspring were raised in a common unstressed environment. We used methylation-sensitive amplified fragment length polymorphism markers to screen genome-wide methylation alterations triggered by stress treatments and to assess the heritability of induced changes. \textbullet{} Various stresses, most notably chemical induction of herbivore and pathogen defenses, triggered considerable methylation variation throughout the genome. Many modifications were faithfully transmitted to offspring. Stresses caused some epigenetic divergence between treatment and controls, but also increased epigenetic variation among plants within treatments. \textbullet{} These results show the following. First, stress-induced methylation changes are common and are mostly heritable. Second, sequence-independent, autonomous methylation variation is readily generated. This highlights the potential of epigenetic inheritance to play an independent role in evolutionary processes, which is superimposed on the system of genetic inheritance.},
  copyright = {\textcopyright{} The Authors (2009). Journal compilation \textcopyright{} New Phytologist (2009)},
  langid = {english},
  keywords = {\#high,abiotic stress,apomixis,DNA methylation,DNAme,epigenetic inheritance,jasmonic acid,methylation-sensitive amplified fragment length polymorphism,plants,salicylic acid,selection,Taraxacum officinale (dandelion)},
  file = {/Users/hawleyj/Documents/Zotero/storage/38J969NS/Verhoeven et al_2010_Stress-induced DNA methylation changes and their heritability in asexual.pdf}
}

