  author = {Hirsch, Cassandra M. and Nazha, Aziz and Kneen, Kassy and Abazeed, Mohamed E. and Meggendorfer, Manja and Przychodzen, Bartlomiej P. and Nadarajah, Niroshan and Adema, Vera and Nagata, Yasunobu and Goyal, Abhinav and Awada, Hassan and Asad, Mohammad Fahad and Visconte, Valeria and Guan, Yihong and Sekeres, Mikkael A. and Olinski, Ryszard and Jha, Babal Kant and LaFramboise, Thomas and Radivoyevitch, Tomas and Haferlach, Torsten and Maciejewski, Jaroslaw P.},
  year = {2018},
