@article{flavahanAlteredChromosomalTopology2019,
  title = {Altered Chromosomal Topology Drives Oncogenic Programs in {{SDH-deficient GIST}}},
  author = {Flavahan, William A. and Drier, Yotam and Johnstone, Sarah E. and Hemming, Matthew L. and Tarjan, Daniel R. and Hegazi, Esmat and Shareef, Sarah J. and Javed, Nauman M. and Raut, Chandrajit P. and Eschle, Benjamin K. and Gokhale, Prafulla C. and Hornick, Jason L. and Sicinska, Ewa T. and Demetri, George D. and Bernstein, Bradley E.},
  year = {2019},
  month = oct,
  journal = {Nature},
  pages = {1--1},
  issn = {1476-4687},
  doi = {10.1038/s41586-019-1668-3},
  abstract = {Epigenetic aberrations are widespread in cancer, yet the underlying mechanisms and causality remain poorly understood1\textendash 3. A subset of gastrointestinal stromal tumors (GISTs) lack canonical kinase mutations but instead have succinate dehydrogenase (SDH)-deficiency and global DNA hyper-methylation4,5. Here we associate this hyper-methylation with changes in genome topology that activate oncogenic programs. To investigate epigenetic alterations systematically, we mapped DNA methylation, CTCF insulators, enhancers, and chromosome topology in KIT-mutant, PDGFRA-mutant, and SDH-deficient GISTs. Although these respective subtypes shared similar enhancer landscapes, we identified hundreds of putative insulators where DNA methylation replaced CTCF binding in SDH-deficient GISTs. We focused on a disrupted insulator that normally partitions a core GIST super-enhancer from the FGF4 oncogene. Recurrent loss of this insulator alters locus topology in SDH-deficient GISTs, allowing aberrant physical interaction between enhancer and oncogene. CRISPR-mediated excision of the corresponding CTCF motifs in an SDH-intact GIST model disrupted the boundary and strongly up-regulated FGF4 expression. We also identified a second recurrent insulator loss event near the KIT oncogene, which is also highly expressed across SDH-deficient GISTs. Finally, we established a patient-derived xenograft (PDX) from an SDH-deficient GIST that faithfully maintains the epigenetics of the parental tumor, including hyper-methylation and insulator defects. This PDX model is highly sensitive to FGF receptor (FGFR) inhibitor, and more so to combined FGFR and KIT inhibition, validating the functional significance of the underlying epigenetic lesions. Our study reveals how epigenetic alterations can drive oncogenic programs in the absence of canonical kinase mutations, with implications for mechanistic targeting of aberrant pathways in cancers.},
  copyright = {2019 The Author(s), under exclusive licence to Springer Nature Limited},
  langid = {english},
  keywords = {\#med,3d-genome,CTCF,DNAme,GIST,Hi-C,phd-thesis,SDH-deficiency,topology},
  file = {/Users/hawleyj/Documents/Zotero/storage/57MU7V73/Flavahan et al. - 2019 - Altered chromosomal topology drives oncogenic prog.pdf}
}

