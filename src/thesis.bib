  author = {Corces, M. Ryan and Buenrostro, Jason D. and Wu, Beijing and Greenside, Peyton G. and Chan, Steven M. and Koenig, Julie L. and Snyder, Michael P. and Pritchard, Jonathan K. and Kundaje, Anshul and Greenleaf, William J. and Majeti, Ravindra and Chang, Howard Y.},
