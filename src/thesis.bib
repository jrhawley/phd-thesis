  author = {Lan, Xiaoyang and J{\"o}rg, David J. and Cavalli, Florence M. G. and Richards, Laura M. and Nguyen, Long V. and Vanner, Robert J. and Guilhamon, Paul and Lee, Lilian and Kushida, Michelle M. and Pellacani, Davide and Park, Nicole I. and Coutinho, Fiona J. and Whetstone, Heather and Selvadurai, Hayden J. and Che, Clare and Luu, Betty and Carles, Annaick and Moksa, Michelle and Rastegar, Naghmeh and Head, Renee and Dolma, Sonam and Prinos, Panagiotis and Cusimano, Michael D. and Das, Sunit and Bernstein, Mark and Arrowsmith, Cheryl H. and Mungall, Andrew J. and Moore, Richard A. and Ma, Yussanne and Gallo, Marco and Lupien, Mathieu and Pugh, Trevor J. and Taylor, Michael D. and Hirst, Martin and Eaves, Connie J. and Simons, Benjamin D. and Dirks, Peter B.},
