}

@article{zhangDNAMethylationAnalysis2009,
  title = {{{DNA Methylation Analysis}} of {{Chromosome}} 21 {{Gene Promoters}} at {{Single Base Pair}} and {{Single Allele Resolution}}},
  author = {Zhang, Yingying and Rohde, Christian and Tierling, Sascha and Jurkowski, Tomasz P. and Bock, Christoph and Santacruz, Diana and Ragozin, Sergey and Reinhardt, Richard and Groth, Marco and Walter, J{\"o}rn and Jeltsch, Albert},
  editor = {Sch{\"u}beler, Dirk},
  year = {2009},
  month = mar,
  journal = {PLoS Genetics},
  volume = {5},
  number = {3},
  pages = {e1000438},
  issn = {1553-7404},
  doi = {10.1371/journal.pgen.1000438},
  abstract = {Differential DNA methylation is an essential epigenetic signal for gene regulation, development, and disease processes. We mapped DNA methylation patterns of 190 gene promoter regions on chromosome 21 using bisulfite conversion and subclone sequencing in five human cell types. A total of 28,626 subclones were sequenced at high accuracy using (longread) Sanger sequencing resulting in the measurement of the DNA methylation state of 580427 CpG sites. Our results show that average DNA methylation levels are distributed bimodally with enrichment of highly methylated and unmethylated sequences, both for amplicons and individual subclones, which represent single alleles from individual cells. Within CpG-rich sequences, DNA methylation was found to be anti-correlated with CpG dinucleotide density and GC content, and methylated CpGs are more likely to be flanked by AT-rich sequences. We observed over-representation of CpG sites in distances of 9, 18, and 27 bps in highly methylated amplicons. However, DNA sequence alone is not sufficient to predict an amplicon's DNA methylation status, since 43\% of all amplicons are differentially methylated between the cell types studied here. DNA methylation in promoter regions is strongly correlated with the absence of gene expression and low levels of activating epigenetic marks like H3K4 methylation and H3K9 and K14 acetylation. Utilizing the single base pair and single allele resolution of our data, we found that i) amplicons from different parts of a CpG island frequently differ in their DNA methylation level, ii) methylation levels of individual cells in one tissue are very similar, and iii) methylation patterns follow a relaxed site-specific distribution. Furthermore, iv) we identified three cases of allele-specific DNA methylation on chromosome 21. Our data shed new light on the nature of methylation patterns in human cells, the sequence dependence of DNA methylation, and its function as epigenetic signal in gene regulation. Further, we illustrate genotype\textendash epigenotype interactions by showing novel examples of allele-specific methylation.},
  langid = {english},
  keywords = {allele-specific methylation; Sanger,DNAme},
  file = {/Users/hawleyj/Documents/Zotero/storage/6GQK723E/zhang2009.pdf}
}

