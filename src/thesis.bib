  langid = {english},
  keywords = {b_all,canalization,Cancer,Cancer epigenetics,chemotherapy,Chemotherapy,genetic_heterogeneity,Haematological cancer,resistance,Tumour heterogeneity},
  annotation = {Bandiera\_abtest: a Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Cancer;Cancer epigenetics;Chemotherapy;Haematological cancer;Tumour heterogeneity Subject\_term\_id: cancer;cancer-epigenetics;chemotherapy;haematological-cancer;tumour-heterogeneity}
