@article{figueroaLeukemicIDH1IDH22010,
  title = {Leukemic {{IDH1}} and {{IDH2 Mutations Result}} in a {{Hypermethylation Phenotype}}, {{Disrupt TET2 Function}}, and {{Impair Hematopoietic Differentiation}}},
  author = {Figueroa, Maria E. and {Abdel-Wahab}, Omar and Lu, Chao and Ward, Patrick S. and Patel, Jay and Shih, Alan and Li, Yushan and Bhagwat, Neha and Vasanthakumar, Aparna and Fernandez, Hugo F. and Tallman, Martin S. and Sun, Zhuoxin and Wolniak, Kristy and Peeters, Justine K. and Liu, Wei and Choe, Sung E. and Fantin, Valeria R. and Paietta, Elisabeth and L{\"o}wenberg, Bob and Licht, Jonathan D. and Godley, Lucy A. and Delwel, Ruud and Valk, Peter J.M. and Thompson, Craig B. and Levine, Ross L. and Melnick, Ari},
