@article{espirituEvolutionaryLandscapeLocalized2018,
  title = {The {{Evolutionary Landscape}} of {{Localized Prostate Cancers Drives Clinical Aggression}}},
  author = {Espiritu, Shadrielle Melijah G. and Liu, Lydia Y. and Rubanova, Yulia and Bhandari, Vinayak and Holgersen, Erle M. and Szyca, Lesia M. and Fox, Natalie S. and Chua, Melvin L. K. and Yamaguchi, Takafumi N. and Heisler, Lawrence E. and Livingstone, Julie and Wintersinger, Jeff and Yousif, Fouad and Lalonde, Emilie and Rouette, Alexandre and Salcedo, Adriana and Houlahan, Kathleen E. and Li, Constance H. and Huang, Vincent and Fraser, Michael and van der Kwast, Theodorus and Morris, Quaid D. and Bristow, Robert G. and Boutros, Paul C.},
