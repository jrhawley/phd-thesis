@article{michorCanChromosomalInstability2005,
  title = {Can Chromosomal Instability Initiate Tumorigenesis?},
  author = {Michor, Franziska and Iwasa, Yoh and Vogelstein, Bert and Lengauer, Christoph and Nowak, Martin A.},
  year = {2005},
