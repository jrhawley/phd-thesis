  langid = {english},
  pmid = {32122974},
  keywords = {2020,canada,cancer,phd-thesis,statistics},
  file = {/Users/hawleyj/Documents/Zotero/storage/974XLDMC/Brenner et al. - 2020 - Projected estimates of cancer in Canada in 2020.pdf}
}

