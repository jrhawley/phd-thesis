  abstract = {Chromatin immunoprecipitation followed by sequencing (ChIP\textendash seq) detects protein\textendash DNA binding events and chemical modifications of histone proteins.Recent technological advances in the ChIP\textendash seq protocol have enabled assaying samples with limited cells, increased precision of the genomic location of binding events, and assaying multiple binding events. However, technical and analytical challenges remain.Open chromatin assays \textemdash{} such as DNase\textendash seq, formaldehyde-assisted identification of regulatory elements (FAIRE\textendash seq) and DNaseI footprinting \textemdash{} offer complementary methods to identify genomic regions bound by regulatory proteins.Chromatin conformation capture (3C) and chromatin interaction analysis with paired-end tag sequencing (ChIA-PET) experiments detect three-dimensional chromatin interactions between bound proteins.Protein binding efficiency varies across sites within a single genome due to differences in the underlying genomic sequences and chromatin state. These differences affect the functionality of transcription factors within cells.SNPs in protein\textendash DNA binding sites can affect binding efficiency across individuals and can be detected by allelic biases in sequences produced from high-throughput sequencing assays.},
  copyright = {2012 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
  langid = {english},
  keywords = {phd-thesis},
  file = {/Users/hawleyj/Documents/Zotero/storage/F7H56IME/Furey - 2012 - ChIP–seq and beyond new and improved methodologie.pdf}
}

@article{furlongDevelopmentalEnhancersChromosome2018,
  title = {Developmental Enhancers and Chromosome Topology},
  author = {Furlong, Eileen E. M. and Levine, Michael},
  year = {2018},
