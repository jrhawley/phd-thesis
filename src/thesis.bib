@article{vanderweideHiCAnalysesGENOVA2021,
  title = {Hi-{{C}} Analyses with {{GENOVA}}: A Case Study with Cohesin Variants},
  shorttitle = {Hi-{{C}} Analyses with {{GENOVA}}},
  author = {{van der Weide}, Robin H and {van den Brand}, Teun and Haarhuis, Judith H I and Teunissen, Hans and Rowland, Benjamin D and {de Wit}, Elzo},
