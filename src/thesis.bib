@article{georgeLeukaemiaCellOrigin2016,
  title = {Leukaemia Cell of Origin Identified by Chromatin Landscape of Bulk Tumour Cells},
  author = {George, Joshy and Uyar, Asli and Young, Kira and Kuffler, Lauren and {Waldron-Francis}, Kaiden and Marquez, Eladio and Ucar, Duygu and Trowbridge, Jennifer J.},
