@article{fuFOXA1OverexpressionMediates2016,
  title = {{{FOXA1}} Overexpression Mediates Endocrine Resistance by Altering the {{ER}} Transcriptome and {{IL-8}} Expression in {{ER-positive}} Breast Cancer},
  author = {Fu, Xiaoyong and Jeselsohn, Rinath and Pereira, Resel and Hollingsworth, Emporia F. and Creighton, Chad J. and Li, Fugen and Shea, Martin and Nardone, Agostina and De Angelis, Carmine and Heiser, Laura M. and Anur, Pavana and Wang, Nicholas and Grasso, Catherine S. and Spellman, Paul T. and Griffith, Obi L. and Tsimelzon, Anna and Gutierrez, Carolina and Huang, Shixia and Edwards, Dean P. and Trivedi, Meghana V. and Rimawi, Mothaffar F. and {Lopez-Terrada}, Dolores and Hilsenbeck, Susan G. and Gray, Joe W. and Brown, Myles and Osborne, C. Kent and Schiff, Rachel},
  year = {2016},
