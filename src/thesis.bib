@article{nguengangwakapEstimatingCumulativePoint2020,
  title = {Estimating Cumulative Point Prevalence of Rare Diseases: Analysis of the {{Orphanet}} Database},
  shorttitle = {Estimating Cumulative Point Prevalence of Rare Diseases},
  author = {Nguengang Wakap, St{\'e}phanie and Lambert, Deborah M. and Olry, Annie and Rodwell, Charlotte and Gueydan, Charlotte and Lanneau, Val{\'e}rie and Murphy, Daniel and Le Cam, Yann and Rath, Ana},
  year = {2020},
