@article{tirado-gonzalezAXLInhibitionMacrophages2021,
  title = {{{AXL Inhibition}} in {{Macrophages Stimulates Host-versus-Leukemia Immunity}} and {{Eradicates Na\"ive}} and {{Treatment-Resistant Leukemia}}},
  author = {{Tirado-Gonzalez}, Irene and Descot, Arnaud and Soetopo, Devona and Nevmerzhitskaya, Aleksandra and Sch{\"a}ffer, Alexander and Kur, Ivan-Maximilano and Czlonka, Ewelina and Wachtel, Carolin and Tsoukala, Ioanna and M{\"u}ller, Luise and Sch{\"a}fer, Anna-Lena and Weitmann, Maresa and Dinse, Petra and Alberto, Emily and Buck, Mich{\`e}le C. and Landry, Jonathan JM and Baying, Bianka and {Slotta-Huspenina}, Julia and Roesler, Jenny and Harter, Patrick N. and Kubasch, Anne-Sophie and Meinel, J{\"o}rn and Elwakeel, Eiman and Strack, Elisabeth and Quang, Christine Tran and {Abdel-Wahab}, Omar and Schmitz, Marc and Weigert, Andreas and Schmid, Tobias and Platzbecker, Uwe and Benes, Vladimir and Ghysdael, Jacques and Bonig, Halvard and G{\"o}tze, Katharina S. and Rothlin, Carla V. and Ghosh, Sourav and Medyouf, Hind},
  year = {2021},
  month = nov,
  journal = {Cancer Discovery},
  volume = {11},
  number = {11},
  pages = {2924--2943},
  publisher = {{American Association for Cancer Research}},
  issn = {2159-8274, 2159-8290},
  doi = {10.1158/2159-8290.CD-20-1378},
  abstract = {Acute leukemias are systemic malignancies associated with a dire outcome. Because of low immunogenicity, leukemias display a remarkable ability to evade immune control and are often resistant to checkpoint blockade. Here, we discover that leukemia cells actively establish a suppressive environment to prevent immune attacks by co-opting a signaling axis that skews macrophages toward a tumor-promoting tissue repair phenotype, namely the GAS6/AXL axis. Using aggressive leukemia models, we demonstrate that ablation of the AXL receptor specifically in macrophages, or its ligand GAS6 in the environment, stimulates antileukemic immunity and elicits effective and lasting natural killer cell\textendash{} and T cell\textendash dependent immune response against na\"ive and treatment-resistant leukemia. Remarkably, AXL deficiency in macrophages also enables PD-1 checkpoint blockade in PD-1\textendash refractory leukemias. Finally, we provide proof-of-concept that a clinical-grade AXL inhibitor can be used in combination with standard-of-care therapy to cure established leukemia, regardless of AXL expression in malignant cells. Significance: Alternatively primed myeloid cells predict negative outcome in leukemia. By demonstrating that leukemia cells actively evade immune control by engaging AXL receptor tyrosine kinase in macrophages and promoting their alternative priming, we identified a target which blockade, using a clinical-grade inhibitor, is vital to unleashing the therapeutic potential of myeloid-centered immunotherapy.This article is highlighted in the In This Issue feature, p. 2659},
  chapter = {Research Articles},
  copyright = {\textcopyright 2021 The Authors; Published by the American Association for Cancer Research. This work is licensed under a Creative Commons Attribution-NonCommercial-NoDerivs International 4.0 License.},
  langid = {english},
  pmid = {34103328}
}

@article{tobiSelectiveSurvivalEmbryos2018,
  title = {Selective {{Survival}} of {{Embryos Can Explain DNA Methylation Signatures}} of {{Adverse Prenatal Environments}}},
  author = {Tobi, Elmar W. and {van den Heuvel}, Joost and Zwaan, Bas J. and Lumey, L. H. and Heijmans, Bastiaan T. and Uller, Tobias},
