@article{laussParentalDNAMethylation2018,
  title = {Parental {{DNA Methylation States Are Associated}} with {{Heterosis}} in {{Epigenetic Hybrids}}},
  author = {Lauss, Kathrin and Wardenaar, Ren{\'e} and Oka, Rurika and van Hulten, Marieke H. A. and Guryev, Victor and Keurentjes, Joost J. B. and Stam, Maike and Johannes, Frank},
