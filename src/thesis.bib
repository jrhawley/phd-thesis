  author = {Chen, Yi-an and Lemire, Mathieu and Choufani, Sanaa and Butcher, Darci T. and Grafodatskaya, Daria and Zanke, Brent W. and Gallinger, Steven and Hudson, Thomas J. and Weksberg, Rosanna},
  year = {2013},
