@article{shihCombinationTargetedTherapy2017,
  title = {Combination Targeted Therapy to Disrupt Aberrant Oncogenic Signaling and Reverse Epigenetic Dysfunction in {{IDH2-}} and {{TET2-mutant}} Acute Myeloid Leukemia},
  author = {Shih, Alan H. and Meydan, Cem and Shank, Kaitlyn and {Garrett-Bakelman}, Francine E. and Ward, Patrick S. and Intlekofer, Andrew M. and Nazir, Abbas and Stein, Eytan M. and Knapp, Kristina and Glass, Jacob and Travins, Jeremy and Straley, Kim and Gliser, Camelia and Mason, Christopher E. and Yen, Katharine and Thompson, Craig B. and Melnick, Ari and Levine, Ross L.},
  year = {2017},
