@article{johnsonSinglecellMultimodalGlioma2021,
  title = {Single-Cell Multimodal Glioma Analyses Identify Epigenetic Regulators of Cellular Plasticity and Environmental Stress Response},
  author = {Johnson, Kevin C. and Anderson, Kevin J. and Courtois, Elise T. and Gujar, Amit D. and Barthel, Floris P. and Varn, Frederick S. and Luo, Diane and Seignon, Martine and Yi, Eunhee and Kim, Hoon and Estecio, Marcos R. H. and Zhao, Dacheng and Tang, Ming and Navin, Nicholas E. and Maurya, Rahul and Ngan, Chew Yee and Verburg, Niels and {de Witt Hamer}, Philip C. and Bulsara, Ketan and Samuels, Michael L. and Das, Sunit and Robson, Paul and Verhaak, Roel G. W.},
