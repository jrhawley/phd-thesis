  author = {Meissner, A and Mikkelsen, T S and Gu, H and Wernig, M and Hanna, J and Sivachenko, A and Zhang, X and Bernstein, B E and Nusbaum, C and Jaffe, D B and Gnirke, A and Jaenisch, R and Lander, E S},
