@article{chadwickDXZ4ChromatinAdopts2008,
  title = {{{DXZ4}} Chromatin Adopts an Opposing Conformation to That of the Surrounding Chromosome and Acquires a Novel Inactive {{X}}-Specific Role Involving {{CTCF}} and Antisense Transcripts},
  author = {Chadwick, Brian P.},
