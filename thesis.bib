@article{singhDisruptingMitochondrialCopper2020,
  title = {Disrupting {{Mitochondrial Copper Distribution Inhibits Leukemic Stem Cell Self}}-{{Renewal}}},
  author = {Singh, Rashim Pal and Jeyaraju, Danny V. and Voisin, Veronique and Hurren, Rose and Xu, Changjiang and Hawley, James R. and Barghout, Samir H. and Khan, Dilshad H. and Gronda, Marcela and Wang, Xiaoming and Jitkova, Yulia and Sharon, David and Liyanagae, Sanduni and MacLean, Neil and Seneviratene, Ayesh K. and Mirali, Sara and Borenstein, Adina and Thomas, Geethu E. and Soriano, Joelle and Orouji, Elias and Minden, Mark D. and Arruda, Andrea and Chan, Steven M. and Bader, Gary D. and Lupien, Mathieu and Schimmer, Aaron D.},
