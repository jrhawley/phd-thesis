}

@article{slaymakerRationallyEngineeredCas92016,
  title = {Rationally Engineered {{Cas9}} Nucleases with Improved Specificity},
  author = {Slaymaker, I. M. and Gao, L. and Zetsche, B. and Scott, D. A. and Yan, W. X. and Zhang, F.},
