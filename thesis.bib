  author = {Duy, Cihangir and Li, Meng and Teater, Matt and Meydan, Cem and {Garrett-Bakelman}, Francine E. and Lee, Tak C. and Chin, Christopher R. and Durmaz, Ceyda and Kawabata, Kimihito C. and Dhimolea, Eugen and Mitsiades, Constantine S. and Doehner, Hartmut and D'Andrea, Richard J. and Becker, Michael W. and Paietta, Elisabeth M. and Mason, Christopher E. and Carroll, Martin and Melnick, Ari M.},
  year = {2021},
  month = jun,
  volume = {11},
