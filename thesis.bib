@article{haTITANInferenceCopy2014,
  title = {{{TITAN}}: Inference of Copy Number Architectures in Clonal Cell Populations from Tumor Whole-Genome Sequence Data.},
  author = {Ha, Gavin and Roth, Andrew and Khattra, Jaswinder and Ho, Julie and Yap, Damian and Prentice, Leah M and Melnyk, Nataliya and McPherson, Andrew and Bashashati, Ali and Laks, Emma and Biele, Justina and Ding, Jiarui and Le, Alan and Rosner, Jamie and Shumansky, Karey and Marra, Marco A and Gilks, C Blake and Huntsman, David G and McAlpine, Jessica N and Aparicio, Samuel and Shah, Sohrab P},
  year = {2014},
  month = nov,
  volume = {24},
  pages = {1881--93},
  doi = {10.1101/gr.180281.114},
  abstract = {The evolution of cancer genomes within a single tumor creates mixed cell populations with divergent somatic mutational landscapes. Inference of tumor subpopulations has been disproportionately focused on the assessment of somatic point mutations, whereas computational methods targeting evolutionary dynamics of copy number alterations (CNA) and loss of heterozygosity (LOH) in whole-genome sequencing data remain underdeveloped. We present a novel probabilistic model, TITAN, to infer CNA and LOH events while accounting for mixtures of cell populations, thereby estimating the proportion of cells harboring each event. We evaluate TITAN on idealized mixtures, simulating clonal populations from whole-genome sequences taken from genomically heterogeneous ovarian tumor sites collected from the same patient. In addition, we show in 23 whole genomes of breast tumors that the inference of CNA and LOH using TITAN critically informs population structure and the nature of the evolving cancer genome. Finally, we experimentally validated subclonal predictions using fluorescence in situ hybridization (FISH) and single-cell sequencing from an ovarian cancer patient sample, thereby recapitulating the key modeling assumptions of TITAN.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\S8FSGB73\\Ha et al._2014_TITAN inference of copy number architectures in clonal cell populations from tumor whole-genome sequence data.pdf},
  journal = {Genome research},
  keywords = {phylogenetic-inference,phylogenetics},
  number = {11}
}

@article{haubenEnergyUseEfficiency2009,
  title = {Energy Use Efficiency Is Characterized by an Epigenetic Component That Can Be Directed through Artificial Selection to Increase Yield},
  author = {Hauben, Miriam and Haesendonckx, Boris and Standaert, Evi and Kelen, Katrien Van Der and Azmi, Abdelkrim and Akpo, Herv{\'e} and Breusegem, Frank Van and Guisez, Yves and Bots, Marc and Lambert, Bart and Laga, Benjamin and Block, Marc De},
  year = {2009},
  month = nov,
  volume = {106},
  pages = {20109--20114},
  issn = {0027-8424, 1091-6490},
  doi = {10/d9vmsb},
  abstract = {Quantitative traits, such as size and weight in animals and seed yield in plants, are distributed normally, even within a population of genetically identical individuals. For example, in plants, various factors, such as local soil quality, microclimate, and sowing depth, affect growth differences among individual plants of isogenic populations. Besides these physical factors, also epigenetic components contribute to differences in growth and yield. The network that regulates crop yield is still not well understood. Although this network is expected to have epigenetic elements, it is completely unclear whether it would be possible to shape the epigenome to increase crop yield. Here we show that energy use efficiency is an important factor in determining seed yield in canola (Brassica napus) and that it can be selected artificially through an epigenetic feature. From an isogenic canola population of which the individual plants and their self-fertilized progenies were recursively selected for respiration intensity, populations with distinct physiological and agronomical characteristics could be generated. These populations were found to be genetically identical, but epigenetically different. Furthermore, both the DNA methylation patterns as well as the agronomical and physiological characteristics of the selected lines were heritable. Hybrids derived from parent lines selected for high energy use efficiencies had a 5\% yield increase on top of heterosis. Our results demonstrate that artificial selection allows the increase of the yield potential by selecting populations with particular epigenomic states.},
  copyright = {\textcopyright{} 2009 .  Freely available online through the PNAS open access option.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\LSFXZ6ZL\\Hauben et al_2009_Energy use efficiency is characterized by an epigenetic component that can be.pdf},
  journal = {Proceedings of the National Academy of Sciences},
  keywords = {\#high,artificial selection,Brassica napus,DNAme,epigenome,plants,selection},
  language = {en},
  number = {47},
  pmid = {19897729}
}

@article{hauserHeterogeneityBackgroundFitness2014,
  title = {Heterogeneity in Background Fitness Acts as a Suppressor of Selection},
  author = {Hauser, Oliver P. and Traulsen, Arne and Nowak, Martin A.},
  year = {2014},
  month = feb,
  volume = {343},
  pages = {178--185},
  issn = {00225193},
  doi = {10.1016/j.jtbi.2013.10.013},
  abstract = {We introduce the concept of heterogeneity in background fitness to evolutionary dynamics in finite populations. Background fitness is specific to an individual but not linked to its strategy. It can be thought of as a property that is related to the physical or societal position of an individual, but is not dependent on the strategy that is adopted in the evolutionary process under consideration. In our model, an individual's total fitness is the sum of its background fitness and the fitness derived from using a specific strategy. This approach has important implications for the imitation of behavioural strategies: if we imitate others for their success, but can only adopt their behaviour and not their social and economic ties, we may imitate in vain. We study the effect of heterogeneity in background fitness on the fixation of a mutant strategy with constant fitness. We find that heterogeneity suppresses selection, but also decreases the time until a novel strategy either takes over the population or is lost again. We derive analytical solutions of the fixation probability in small populations. In the case of large total background fitness in a population with maximum inequality, we find a particularly simple approximation of the fixation probability. Numerical simulations suggest that this simple approximation also holds for larger population sizes.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\9M5SAVUU\\Hauser et al. - 2014 - Heterogeneity in background fitness acts as a supp.pdf},
  journal = {Journal of Theoretical Biology},
  keywords = {\#med},
  language = {en}
}

@article{hawleyCisRegulatoryElementHijacking2021,
  title = {Cis-{{Regulatory Element Hijacking}} by {{Structural Variants Overshadows Topological Changes}} in {{Primary Prostate Cancer}}},
  author = {Hawley, James R. and Zhou, Stanley and Arlidge, Christopher and Grillo, Giacomo and Kron, Ken and {Hugh-White}, Rupert and van der Kwast, Theodorus and Fraser, Michael and Boutros, Paul C. and Bristow, Robert G. and Lupien, Mathieu},
  year = {2021},
  month = jan,
  pages = {2021.01.05.425333},
  publisher = {{Cold Spring Harbor Laboratory}},
  doi = {10.1101/2021.01.05.425333},
  abstract = {Prostate cancer is a heterogeneous disease whose progression is linked to genome instability. Despite large-scale tumour sequencing efforts, the impact of mutations on the genetic architecture in cancer remains ill-defined due to limited integration of genomics data across dimensions. We addressed this limitation by assessing the impact of structural variants on the chromatin states and the three-dimensional organization of the primary prostate cancer genome. We find high concordance in the three-dimensional genome organization between tumours and benign prostate tissues, arguing for constraints to the three-dimensional genome of prostate tumours. Moreover, we identify structural variants as effectors of changes to chromatin interactions, guiding cis-regulatory element hijacking that imposes antipode expression on genes flanking the breakpoints. This leads to the repression of tumour suppressor gene expression and up-regulation of oncogenes, such as seen for the TMPRSS2-ERG and PMEPA1-ZNF156 loci. Collectively, our results argue that cis-regulatory element hijacking overshadows the impact of structural variants in gene regulation alterations to promote oncogenesis.},
  chapter = {New Results},
  copyright = {\textcopyright{} 2021, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NonCommercial 4.0 International), CC BY-NC 4.0, as described at http://creativecommons.org/licenses/by-nc/4.0/},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\3BB5BZ3M\\Hawley et al. - 2021 - Cis-Regulatory Element Hijacking by Structural Var.pdf;C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\RUSMDFJ7\\Cis-Regulatory Element Hijacking Overshadows Topological Changes in Prostate Cancer - Hawley, Zhou, et al.pdf},
  journal = {bioRxiv},
  language = {en}
}

@article{hawsMethylMetaboliteDepletionElicits2019,
  title = {Methyl-{{Metabolite Depletion Elicits Adaptive Responses}} to {{Support Heterochromatin Stability}} and {{Epigenetic Persistence}}},
  author = {Haws, Spencer A. and Yu, Deyang and Ye, Cunqi and Wille, Coral K. and Nguyen, Long C. and Krautkramer, Kimberly A. and Tomasiewicz, Jay L. and Yang, Shany E. and Miller, Blake R. and Liu, Wallace H. and Igarashi, Kazuhiko and Sridharan, Rupa and Tu, Benjamin P. and Cryns, Vincent L. and Lamming, Dudley W. and Denu, John M.},
  year = {2019},
  month = aug,
  doi = {10.1101/726448},
  abstract = {Summary                        S-adenosylmethionine (SAM) is the methyl-donor substrate for DNA and histone methyltransferases that regulate cellular epigenetic states. This metabolism-epigenome link enables the sensitization of chromatin methylation to altered SAM abundance. However, a chromatin-wide understanding of the adaptive/responsive mechanisms that allow cells to actively protect epigenetic information during life-experienced fluctuations in SAM availability are unknown. We identified a robust response to SAM depletion that is highlighted by preferential cytoplasmic and nuclear             de novo             mono-methylation of H3 Lys 9 (H3K9) at the expense of global losses in histone di- and tri-methylation. Under SAM-depleted conditions,             de novo             H3K9 mono-methylation preserves heterochromatin stability and supports global epigenetic persistence upon metabolic recovery. This unique chromatin response was robust across the mouse lifespan and correlated with improved metabolic health, supporting a significant role for epigenetic adaptation to SAM depletion             in vivo             . Together, these studies provide the first evidence for active epigenetic adaptation and persistence to metabolic stress.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\BU8EWYFL\\Haws et al. - 2019 - Methyl-Metabolite Depletion Elicits Adaptive Respo.pdf;C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\HKJXUVRK\\media-1.xlsx;C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\NGGMF6GZ\\media-3.xlsx;C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\NQUR4MKR\\media-4.pdf;C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\T2ZYEDJF\\media-2.xlsx},
