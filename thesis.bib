@article{dodgetInactivationDnmt3bMouse2005,
  title = {Inactivation of {{Dnmt3b}} in Mouse Embryonic Fibroblasts Results in {{DNA}} Hypomethylation, Chromosomal Instability, and Spontaneous Immortalization},
  author = {Dodget, Jonathan E. and Okano, Masaki and Dick, Fred and Tsujimoto, Naomi and Chen, Taiping and Wang, Shumei and Ueda, Yoshihide and Dyson, Nick and Li, En},
  year = {2005},
  volume = {280},
