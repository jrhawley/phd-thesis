  keywords = {breast cancer,cistrome,ERBB2,estrogen receptor,growth factors,transcription},
  language = {en},
  number = {19},
  pmid = {20889718}
}

@article{luppinoCohesinPromotesStochastic2020,
  title = {Cohesin Promotes Stochastic Domain Intermingling to Ensure Proper Regulation of Boundary-Proximal Genes},
  author = {Luppino, Jennifer M. and Park, Daniel S. and Nguyen, Son C. and Lan, Yemin and Xu, Zhuxuan and Yunker, Rebecca and Joyce, Eric F.},
