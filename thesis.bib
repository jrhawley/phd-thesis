@article{takamiyaRestrictionLandmarkGenome2006,
  title = {Restriction Landmark Genome Scanning Method Using Isoschizomers ({{Mspl}}/{{Hpall}}) for {{DNA}} Methylation Analysis},
  author = {Takamiya, Tomoko and Hosobuchi, Saeko and Asai, Kenji and Nakamura, Eiji and Tomioka, Keisuke and Kawase, Makoto and Kakutani, Tetsuji and Paterson, Andrew H. and Murakami, Yasufumi and Okuizumi, Hisato},
