  copyright = {\textcopyright 2021 American Association for Cancer Research.},
  file = {D\:\\Cloud\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\N7SMHBAH\\Duy et al. - 2021 - Chemotherapy Induces Senescence-Like Resilient Cel.pdf},
