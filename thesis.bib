@article{sallariConvergenceDispersedRegulatory2016,
  title = {Convergence of Dispersed Regulatory Mutations Predicts Driver Genes in Prostate Cancer},
  author = {Sallari, Richard C and {Sinnott-Armstrong}, Nicholas A and French, Juliet D and Kron, Ken J and Ho, Jason and Moore, Jason H and Stambolic, Vuk and Edwards, Stacey L and Lupien, Mathieu and Kellis, Manolis},
  year = {2016},
