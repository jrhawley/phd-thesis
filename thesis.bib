@article{zafarSiCloneFitBayesianInference2019,
  title = {{{SiCloneFit}}: {{Bayesian}} Inference of Population Structure, Genotype, and Phylogeny of Tumor Clones from Single-Cell Genome Sequencing Data},
  shorttitle = {{{SiCloneFit}}},
  author = {Zafar, Hamim and Navin, Nicholas and Chen, Ken and Nakhleh, Luay},
  year = {2019},
  month = nov,
  volume = {29},
  pages = {1847--1859},
  issn = {1088-9051, 1549-5469},
  doi = {10.1101/gr.243121.118},
  abstract = {Accumulation and selection of somatic mutations in a Darwinian framework result in intra-tumor heterogeneity (ITH) that poses significant challenges to the diagnosis and clinical therapy of cancer. Identification of the tumor cell populations (clones) and reconstruction of their evolutionary relationship can elucidate this heterogeneity. Recently developed single-cell DNA sequencing (SCS) technologies promise to resolve ITH to a single-cell level. However, technical errors in SCS data sets, including false-positives (FP) and false-negatives (FN) due to allelic dropout, and cell doublets, significantly complicate these tasks. Here, we propose a nonparametric Bayesian method that reconstructs the clonal populations as clusters of single cells, genotypes of each clone, and the evolutionary relationship between the clones. It employs a tree-structured Chinese restaurant process as the prior on the number and composition of clonal populations. The evolution of the clonal populations is modeled by a clonal phylogeny and a finite-site model of evolution to account for potential mutation recurrence and losses. We probabilistically account for FP and FN errors, and cell doublets are modeled by employing a Beta-binomial distribution. We develop a Gibbs sampling algorithm comprising partial reversible-jump and partial Metropolis-Hastings updates to explore the joint posterior space of all parameters. The performance of our method on synthetic and experimental data sets suggests that joint reconstruction of tumor clones and clonal phylogeny under a finite-site model of evolution leads to more accurate inferences. Our method is the first to enable this joint reconstruction in a fully Bayesian framework, thus providing measures of support of the inferences it makes.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\QC87LJXR\\Zafar et al_2019_SiCloneFit.pdf},
  journal = {Genome Research},
  keywords = {bayesian-inference,phylogeny,tumour-phylogeny},
  language = {en},
  number = {11},
  pmid = {31628257}
}

@article{zaimiVariationDNAMethylation2018,
  title = {Variation in {{DNA}} Methylation of Human Blood over a 1-Year Period Using the {{Illumina MethylationEPIC}} Array},
  author = {Zaimi, Ina and Pei, Dong and Koestler, Devin C. and Marsit, Carmen J. and Vivo, Immaculata De and Tworoger, Shelley S. and Shields, Alexandra E. and Kelsey, Karl T. and Michaud, Dominique S.},
  year = {2018},
  month = nov,
  volume = {13},
  pages = {1056--1071},
  issn = {1559-2294},
  doi = {10/gfpftg},
  abstract = {Assessing DNA methylation profiles in human blood has become a major focus of epidemiologic inquiry. Understanding variability in CpG-specific DNA methylation over moderate periods of time is a critical first step in identifying CpG sites that are candidates for DNA methylation-based etiologic, diagnostic and prognostic predictors of pathogenesis. Using the Illumina MethylationEPIC [850K] BeadArray, DNA methylation was profiled in paired whole blood samples collected approximately 1 year apart from 35 healthy women enrolled in the Nurses Study II cohort. The median intraclass correlation coefficient (ICC) across all CpG loci was 0.19 [Interquartile Range (IQR) 0.00\textendash 0.50]; 74.8\% of ICCs were in the low range (0\textendash 0.5), 16.9\% in the mid-range of ICCs (0.5\textendash 0.8), and 8.3\% in the high-range of ICCs (0.8\textendash 1). ICCs were similar for CpG probes on the 450K Illumina array (median 0.17) and the new probes added to the 850K array (median 0.21). ICCs for CpG loci on the sex chromosomes and known metastable epialleles were high (median 0.71, 0.97, respectively), and ICCs among methylation quantitative trait loci (mQTL) CpGs were significantly higher as compared to non-mQTL CpGs (median 0.73, 0.16, respectively, P {$<$} 2 \texttimes{} 10\textendash 16). We observed wide variation in DNA methylation stability over a 1-year period. Probes considered non-stable, due to substantial variation over a moderate period of time and with minimal variability across individuals could be removed in large epidemiological studies. Moreover, adjusting for technical variation that arises from using high-dimensional arrays is critical.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\4247QXUZ\\Zaimi et al_2018_Variation in DNA methylation of human blood over a 1-year period using the.pdf},
  journal = {Epigenetics},
  keywords = {\#low,DNA methylation,DNAme,DNAme arrays,Illumina MethylationEPIC array,reproducibility,time series,variation},
  number = {10-11},
  pmid = {30270718}
}

@article{zenkHP1DrivesNovo2021,
  title = {{{HP1}} Drives de Novo {{3D}} Genome Reorganization in Early {{Drosophila}} Embryos},
  author = {Zenk, Fides and Zhan, Yinxiu and Kos, Pavel and L{\"o}ser, Eva and Atinbayeva, Nazerke and Sch{\"a}chtle, Melanie and Tiana, Guido and Giorgetti, Luca and Iovino, Nicola},
  year = {2021},
  month = apr,
  issn = {0028-0836, 1476-4687},
  doi = {10.1038/s41586-021-03460-z},
  abstract = {Abstract                            Fundamental features of 3D genome organization are established de novo in the early embryo, including clustering of pericentromeric regions, the folding of chromosome arms and the segregation of chromosomes into active (A-) and inactive (B-) compartments. However, the molecular mechanisms that drive de novo organization remain unknown               1,2               . Here, by combining chromosome conformation capture (Hi-C), chromatin immunoprecipitation with high-throughput sequencing (ChIP\textendash seq), 3D DNA fluorescence in situ hybridization (3D DNA FISH) and polymer simulations, we show that heterochromatin protein 1a (HP1a) is essential for de novo 3D genome organization during               Drosophila               early development. The binding of HP1a at pericentromeric heterochromatin is required to establish clustering of pericentromeric regions. Moreover, HP1a binding within chromosome arms is responsible for overall chromosome folding and has an important role in the formation of B-compartment regions. However, depletion of HP1a does not affect the A-compartment, which suggests that a different molecular mechanism segregates active chromosome regions. Our work identifies HP1a as an epigenetic regulator that is involved in establishing the global structure of the genome in the early embryo.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\L3CND6FS\\Zenk et al. - 2021 - HP1 drives de novo 3D genome reorganization in ear.pdf},
  journal = {Nature},
  language = {en}
}

@article{zhangAcetylationEnhancesTET22017,
  title = {Acetylation {{Enhances TET2 Function}} in {{Protecting}} against {{Abnormal DNA Methylation}} during {{Oxidative Stress}}},
  author = {Zhang, Yang W. and Wang, Zhihong and Xie, Wenbing and Cai, Yi and Xia, Limin and Easwaran, Hariharan and Luo, Jianjun and Yen, Ray-Whay Chiu and Li, Yana and Baylin, Stephen B.},
