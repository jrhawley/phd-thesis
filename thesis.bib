@article{esteveDirectInteractionDNMT12006,
  title = {Direct Interaction between {{DNMT1}} and {{G9a}} Coordinates {{DNA}} and Histone Methylation during Replication},
  author = {Esteve, P.-O. and Chin, H. G. and Smallwood, Andrea and Feehery, George R. and Gangisetty, Omkaram and Karpf, Adam R. and Carey, Michael F. and Pradhan, Sriharsa},
