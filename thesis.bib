  title = {Epigenetic {{Patterns}} in a {{Complete Human Genome}}},
  author = {Gershman, Ariel and Sauria, Michael E.G. and Hook, Paul W. and Hoyt, Savannah J. and Razaghi, Roham and Koren, Sergey and Altemose, Nicolas and Caldas, Gina V. and Vollger, Mitchell R. and Logsdon, Glennis A. and Rhie, Arang and Eichler, Evan E. and Schatz, Michael C. and O'Neill, Rachel J. and Phillippy, Adam M. and Miga, Karen H. and Timp, Winston},
