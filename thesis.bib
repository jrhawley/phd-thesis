  doi = {10/f4h2mh},
  abstract = {Heritable variation in plant phenotypes, and thus potential for evolutionary change, can in principle not only be caused by variation in DNA sequence, but also by underlying epigenetic variation. However, the potential scope of such phenotypic effects and their evolutionary significance are largely unexplored. Here, we conducted a glasshouse experiment in which we tested the response of a large number of epigenetic recombinant inbred lines (epiRILs) of Arabidopsis thaliana \textendash{} lines that are nearly isogenic but highly variable at the level of DNA methylation \textendash{} to drought and increased nutrient conditions. We found significant heritable variation among epiRILs both in the means of several ecologically important plant traits and in their plasticities to drought and nutrients. Significant selection gradients, that is, fitness correlations, of several mean traits and plasticities suggest that selection could act on this epigenetically based phenotypic variation. Our study provides evidence that variation in DNA methylation can cause substantial heritable variation of ecologically important plant traits, including root allocation, drought tolerance and nutrient plasticity, and that rapid evolution based on epigenetic variation alone should thus be possible.},
  copyright = {\textcopyright{} 2012 The Authors New Phytologist \textcopyright{} 2012 New Phytologist Trust},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\BIIASKAX\\Zhang et al_2013_Epigenetic variation creates potential for evolution of plant phenotypic.pdf;C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\KF6J2LDA\\Zhang et al_2013_Epigenetic variation creates potential for evolution of plant phenotypic.pdf},
  journal = {New Phytologist},
  keywords = {\#low,\#med,Arabidopsis thaliana,DNA methylation,DNAme,drought tolerance,ecological epigenetics,epigenetic variation,epiRILs,evolution,heritability,plants,plasticity},
  language = {en},
  number = {1}
}

