@article{szaboRegulationSinglecellGenome2020,
  title = {Regulation of Single-Cell Genome Organization into {{TADs}} and Chromatin Nanodomains},
  author = {Szabo, Quentin and Donjon, Axelle and Jerkovi{\'c}, Ivana and Papadopoulos, Giorgio L. and Cheutin, Thierry and Bonev, Boyan and Nora, Elph{\`e}ge P. and Bruneau, Benoit G. and Bantignies, Fr{\'e}d{\'e}ric and Cavalli, Giacomo},
