@article{allumCharacterizationFunctionalMethylomes2015,
  title = {Characterization of Functional Methylomes by Next-Generation Capture Sequencing Identifies Novel Disease-Associated Variants},
  author = {Allum, Fiona and Shao, Xiaojian and Gu{\'e}nard, Fr{\'e}d{\'e}ric and Simon, Marie-Michelle and Busche, Stephan and Caron, Maxime and Lambourne, John and Lessard, Julie and Tandre, Karolina and Hedman, {\AA}sa K. and Kwan, Tony and Ge, Bing and R{\"o}nnblom, Lars and McCarthy, Mark I. and Deloukas, Panos and Richmond, Todd and Burgess, Daniel and Spector, Timothy D. and Tchernof, Andr{\'e} and Marceau, Simon and Lathrop, Mark and Vohl, Marie-Claude and Pastinen, Tomi and Grundberg, Elin},
