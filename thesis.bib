@article{imDNMT3AIDHMutations2014,
  title = {{{DNMT3A}} and {{IDH}} Mutations in Acute Myeloid Leukemia and Other Myeloid Malignancies: Associations with Prognosis and Potential Treatment Strategies},
  shorttitle = {{{{\emph{DNMT3A}}}} and {{{\emph{IDH}}}} Mutations in Acute Myeloid Leukemia and Other Myeloid Malignancies},
