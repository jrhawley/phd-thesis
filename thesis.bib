@article{jiangEnvironmentallyResponsiveGenomewide2014,
  title = {Environmentally Responsive Genome-Wide Accumulation of de Novo {{Arabidopsis}} Thaliana Mutations and Epimutations},
  author = {Jiang, Caifu and Mithani, Aziz and Belfield, Eric J. and Mott, Richard and Hurst, Laurence D. and Harberd, Nicholas P.},
  year = {2014},
