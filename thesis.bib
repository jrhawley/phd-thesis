  author = {Bhasin, Jeffrey M. and Lee, Byron H. and Matkin, Lars and Taylor, Margaret G. and Hu, Bo and Xu, Yaomin and {Magi-Galluzzi}, Cristina and Klein, Eric A. and Ting, Angela H.},
  year = {2015},
  month = dec,
  volume = {13},
  pages = {2135--2146},
  issn = {2211-1247},
  doi = {10/gft5zt},
  abstract = {Summary A critical need in understanding the biology of prostate cancer is characterizing the molecular differences between indolent and aggressive cases. Because DNA methylation can capture the regulatory state of tumors, we analyzed differential methylation patterns genome-wide among benign prostatic tissue and low-grade and high-grade prostate cancer and found extensive, focal hypermethylation regions unique to high-grade disease. These hypermethylation regions occurred not only in the promoters of genes but also in gene bodies and at intergenic regions that are enriched for DNA-protein binding sites.~Integration with existing RNA-sequencing (RNA-seq)~and survival data revealed regions where DNA methylation correlates with reduced gene expression associated with poor outcome. Regions specific to aggressive disease are proximal to genes with distinct functions from regions shared by indolent and aggressive disease. Our compendium of methylation changes reveals crucial molecular distinctions between indolent and aggressive prostate cancer.},
  file = {C\:\\Users\\james\\OneDrive - University of Toronto\\Apps\\Zotero\\storage\\8HLGUJFY\\Bhasin et al_2015_Methylome-wide Sequencing Detects DNA Hypermethylation Distinguishing Indolent.pdf},
  journal = {Cell Reports},
  number = {10}
}

